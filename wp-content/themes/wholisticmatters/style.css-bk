.dropdown-toggle::after {
	content: "";
	position: absolute;
	top: 17px;
	background: url(images/DropdownArrow.svg);
	background-repeat: no-repeat;
	border: none;
	width: 13px;
	height: 7px;
	margin-left: 0.555em;
}

.navbar-expand-lg .navbar-nav .nav-link {
	padding-right: 2rem;
	padding-left: 2rem;
}


.navbar-expand-lg .navbar-nav li.dropdown.full.show {
	position: static;
}

.dropdown-menu.show {
	display: block;
	width: 100%;
	border-radius: 0;
	top: 46px;
	border: none;
	z-index: 1;
	padding: .5rem 0 1rem;
	box-shadow: inset 0 7px 9px -7px rgba(0, 0, 0, 0.2);
	min-width: 200px;
	border-radius: 0 0 5px 5px;
	-webkit-box-shadow: 0px 5px 7px 0 rgba(0, 0, 0, 0.2);
	box-shadow: 0px 5px 7px 0 rgba(0, 0, 0, 0.2);
}

.show .nav-link::before {
	content: "";
	position: absolute;
	border-bottom: 3px solid #0A9349;
	bottom: -10px;
	left: 0;
	right: 0;
	max-width: 65px;
	margin: 0 auto;
}

.show.full .nav-link::before {
	content: "";
	position: absolute;
	border-bottom: 3px solid #0a9349;
	bottom: -10px;
	left: 27px;
	right: 0;
	max-width: 65px;
	margin: 0 auto;
}

.show.full a {
	position: relative;
}

