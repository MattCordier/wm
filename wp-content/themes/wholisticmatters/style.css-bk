nav.main_nav {
	margin-top: 20px;

}

nav.main_nav ul {
	list-style: none;
	padding: 0;
	margin: 0;
	display: flex;
}

nav.main_nav ul li {
	display: inline-block;
	margin: auto;
}

nav.main_nav ul li a {
	font-size: 14px;
	color: #52565F;
	font-weight: 400;
	transition: all ease 0.5s;
	padding: 10px 15px;
	text-transform: captialize;
	position: relative;
	font-family: 'Aktiv';

}

nav.main_nav ul li.menu-item-has-children {
	position: relative;
	padding: 0 5px 0 0;
}

nav.main_nav ul li.menu-item-has-children:after {
	content: "";
	right: 0;
	position: absolute;
	top: 11px;
	color: #AEAEAE;
	font-size: 20px;
	opacity: 1;
	background: url(images/DropdownArrow.svg);
	background-repeat: no-repeat;
	width: 13px;
	height: 7px;

}

nav.main_nav ul li a:after {
	/*	content:"/";
	color:#000;
	position:absolute;
	right:0;
	font-size:15px;*/
}

nav.main_nav ul li:last-child a:after {
	color: #fff;
	display: none;
}

nav.main_nav ul li a:hover {
	text-decoration: none;
}

.header_right_mod {
	float: right;
	margin-top: 15px;
}

.header_right_mod a {
	text-transform: uppercase;
	border: 1px solid #dcdcdc;
