/**Typogrphy**/
.banner_heading {
	font-family: 'Harriet';
	font-size: 36px;
	font-weight: 700;
	color: #1A4D2C;
}

.section_heading {
	font-size: 24px;
	position: relative;
	color: #40473E;
	font-weight: 900;
	font-family: 'Aktiv';
	padding: 20px 0;
}

.section_heading:before {
	content: "";
	width: 100%;
	height: 2px;
	background: #DDDDDD;
	position: absolute;
	bottom: 0;
	left: 0;
}

.section_heading:after {
	content: "";
	width: 58px;
	height: 6px;
	background: #DDDDDD;
	position: absolute;
	bottom: -2px;
	left: 0;
}

