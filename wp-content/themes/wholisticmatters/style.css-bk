/**Elements**/
.cta_filled {
	color: #fff;
	font-size: 16px;
	background: #00B162;
	border-radius: 5px;
	padding: 8px 30px;
	display: inline-block;
	transition: all ease 0.5s;
	border: 2px solid #00B162;
}

.cta_filled:hover {
	text-decoration: none;
	color: #fff;
	background: #04a25c;
